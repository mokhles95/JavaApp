<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Bookmark.AddBookmarkController">
   <children>
      <GridPane alignment="BOTTOM_RIGHT" layoutX="274.0" layoutY="145.0" prefHeight="41.0" prefWidth="212.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <JFXButton fx:id="cancelAdd" accessibleRole="IMAGE_VIEW" onAction="#cancelAdd" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: #DD4B39; -fx-text-fill: white; -fx-font-size: 14;" text="Cancel" GridPane.columnIndex="1" />
            <JFXButton fx:id="addBookmarkButton" onAction="#addBookmark" prefHeight="10.0" prefWidth="98.0" style="-fx-background-color: #26B99A; -fx-text-fill: white; -fx-font-size: 14;" text="Ok">
               <graphic>
                  <FontAwesomeIconView fill="#fcf8f8" glyphName="CHECK" />
               </graphic>
            </JFXButton>
         </children>
      </GridPane>
      <Label fx:id="dateValue" layoutX="310.0" layoutY="90.0" text="Date">
         <font>
            <Font name="Arial Bold" size="17.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="49.0" layoutY="40.0" text="Project">
         <font>
            <Font name="Arial Bold" size="17.0" />
         </font>
      </Label>
      <Label fx:id="dateLabel" layoutX="310.0" layoutY="40.0" text="Date">
         <font>
            <Font name="Arial Bold" size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
